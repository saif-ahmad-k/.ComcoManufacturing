        <Button Click="ViewCheckSheet" Content="View" Style="{StaticResource normalButton}" HorizontalAlignment="Left" Margin="515,224,0,0" VerticalAlignment="Top" Width="169" Height="44"/>
        <Button Click="EditCheckSheet" Content="Edit" Style="{StaticResource normalButton}" HorizontalAlignment="Left" Margin="515,273,0,0" VerticalAlignment="Top" Width="169" Height="44"/>
