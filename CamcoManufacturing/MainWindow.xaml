        <Image HorizontalAlignment="Left" Source="Images/Camco_Manufacturing_Unofficial_Logo_Large.png" Height="142" VerticalAlignment="Top" Width="224" Margin="0,5,0,0"/>
        <Label Content="PROCESSING"  Style="{StaticResource labelNavText}" HorizontalAlignment="Left" Margin="765,134,0,0" VerticalAlignment="Top" Height="46" Width="169"/>
        <Button Click="Button_Click_1" Content="ADD/EDIT TOOLS" Style="{StaticResource normalButton}" HorizontalAlignment="Left" Margin="765,224,0,0" VerticalAlignment="Top" Width="169" Height="44"/>

