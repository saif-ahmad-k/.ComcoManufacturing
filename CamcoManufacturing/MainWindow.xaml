    <Grid>
        <Image HorizontalAlignment="Left" Source="Images/CamcoBannerBackround.jpg" Height="104" VerticalAlignment="Top" Width="1418" Margin="0,5,-504,0"/>

        <!--<Menu Height="31" Margin="10,0" VerticalAlignment="Top">
            <MenuItem Header="Camco Sheet" Name="sheet" Click="Sheet_Click"/>
            <MenuItem Header="MCS" Name="MCS" Click="MCS_Click"/>
        </Menu>-->
        <Button Click="ViewODSDrawing" Content="View" Style="{StaticResource normalButton}" HorizontalAlignment="Left" Margin="38,224,0,0" VerticalAlignment="Top" Width="169" Height="44"/>
        <Button Click="EditODSDrawing" Content="Edit" Style="{StaticResource normalButton}" HorizontalAlignment="Left" Margin="38,273,0,0" VerticalAlignment="Top" Width="169" Height="44"/>
        <Button Click="CreateODSDrawing" Content="Create" Style="{StaticResource normalButton}" HorizontalAlignment="Left" Margin="38,322,0,0" VerticalAlignment="Top" Width="169" Height="44"/>
