        <Rectangle Fill="Black" HorizontalAlignment="Left" Height="46" Margin="38,134,0,0" Stroke="Black" VerticalAlignment="Top" Width="896" OpacityMask="Black"/>
        <Label Content="ODS DRAWING" Style="{StaticResource labelNavText}" HorizontalAlignment="Left" Margin="38,134,0,0" VerticalAlignment="Top" Height="46" Width="169"/>
